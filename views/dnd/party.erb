<h1>Super Awesome DND Party</h1>
<table class="table">
  <tr>
    <th>ID</th>
    <th>Name</th>
    <th>Class</th>
    <th>Level</th>
  </tr>
  <% locals[:party].each do |member| %>
    <tr>
      <td><%= member[:id] %></td>
      <td><%= member[:name] %></td>
      <td><%= member[:class] %></td>
      <td><%= member[:level] %></td>
    </tr>
  <% end %>
</table>

<p><a href="/dnd/join">Interested in joining?</a></p>
